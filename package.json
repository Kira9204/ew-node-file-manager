{
  "name": "ew-ts-filemanager",
  "version": "1.0.0",
  "description": "A file-manager using react+express written in typescript",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node ./server/www.js",
    "server": "nodemon ./server/www.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run clean && npm run compile",
    "compile": "tsc -p ./server/tsconfig.json",
    "clean": "rimraf ./server/dist/*",
    "watch": "tsc -p ./server/tsconfig.json --watch",
    "tslint": "tslint './src/**/*.{ts,tsx}'",
    "tslint:fix": "npm run tslint -- --fix",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "prettier": "prettier --write --config ./.prettierrc.js **/*.ts"
  },
  "author": "Erik Welander",
  "license": "MIT",
  "devDependencies": {
    "@types/archiver": "^3.0.0",
    "@types/busboy": "^0.2.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-fileupload": "^1.1.0",
    "@types/filesize": "^4.2.0",
    "@types/image-size": "^0.7.0",
    "@types/md5-file": "^4.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/morgan": "^1.7.37",
    "@types/rimraf": "^2.0.3",
    "@types/sharp": "^0.22.3",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "dependencies": {
    "archiver": "^3.1.1",
    "busboy": "^0.3.1",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "diskusage": "^1.1.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "image-size": "^0.8.2",
    "md5-file": "^4.0.0",
    "mime-types": "^2.1.24",
    "morgan": "^1.9.1",
    "npm": "^6.11.3",
    "sharp": "^0.23.1"
  }
}
