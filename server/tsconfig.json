{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es5",
    "esModuleInterop": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "removeComments": true,
    "moduleResolution": "node",
    "declaration": false
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
